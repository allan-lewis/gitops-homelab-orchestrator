services:
  twingate-connector:
    image: twingate/connector:1.80.0
    container_name: valiant_stingray
    restart: unless-stopped
    environment:
      - TWINGATE_NETWORK=${TWINGATE_NETWORK_NAME}
      - TWINGATE_ACCESS_TOKEN=${VALIANT_STINGRAY_ACCESS_TOKEN}
      - TWINGATE_REFRESH_TOKEN=${VALIANT_STINGRAY_REFRESH_TOKEN}

